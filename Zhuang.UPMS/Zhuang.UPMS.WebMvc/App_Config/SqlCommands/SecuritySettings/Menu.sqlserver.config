<?xml version="1.0" encoding="utf-8" ?>
<commands>
  <command key="SecuritySettings.Menu.List">
    <![CDATA[
SELECT  a.MenuId ,
        a.ParentId ,
        a.Name ,
        a.Url ,
        a.[Order] ,
        a.IsExpand ,
        a.RecordStatus ,
        a.CreatedById ,
        a.ModifiedById ,
        a.CreationDate ,
        a.ModifiedDate FROM dbo.Sec_Menu a
WHERE a.ParentId=#ParentId#
{?and a.Name like '%'+#Name#+'%'}
    ]]>
  </command>

</commands>
